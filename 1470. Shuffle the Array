class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        vector<int> ans;
        int i=0;
        // while((i+n)!=nums.size()){
        //     ans.push_back(nums[i]);
        //     ans.push_back(nums[i+n]);
        //     i++;
        // }
        // return ans;
        
        for(int i=0; i<n; i++)
        {
            ans.push_back(nums[i]);
            ans.push_back(nums[i+n]);
        }
        return ans;
    }
};
