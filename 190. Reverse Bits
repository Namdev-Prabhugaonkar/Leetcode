class Solution {
public:
    uint32_t reverseBits(uint32_t n) {
        uint32_t ans=0;
        
        for(int i=0;i<32;i++){
            uint32_t temp=0;
            temp=n&1;
            if(temp){
                ans|=(1<<(31-i));
            }
            n=n>>1;
        }
        return ans;
    }
};
